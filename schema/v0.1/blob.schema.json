{
  "$id": "https://aips.tematom.com/schema/v0.1/blob.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "required": ["aips","type","ts","data","mime"],
  "properties": {
    "aips": { "const": "0.1" },
    "type": { "const": "blob" },
    "id": { "type": "string" },
    "ts": { "type": "string", "format": "date-time" },
    "stream": { "type": "string" },
    "subject": { "type": "string" },
    "mime": { "type": "string" },
    "data": {
      "type": "object",
      "properties": {
        "ref": { "type": "string" },
        "hash": { "type": "string" }
      },
      "additionalProperties": true
    }
  },
  "additionalProperties": true
}
